{
  "name": "ExampleJenkins",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android:dev": "react-native run-android --variant=devDebug",
    "android:dev-release": "react-native run-android --variant=devRelease",
    "android:stg": "react-native run-android --variant=stgDebug",
    "android:stg-release": "react-native run-android --variant=stgRelease",
    "android:uat": "react-native run-android --variant=uatDebug",
    "android:uat-release": "react-native run-android --variant=uatRelease",
    "android:prod": "react-native run-android --variant=prodDebug",
    "android:prod-release": "react-native run-android --variant=prodRelease",
    "ios:dev": "react-native run-ios --scheme ExampleJenkinsDev --mode DevDebug",
    "ios:dev-release": "react-native run-ios --scheme ExampleJenkinsDev --mode DevRelease",
    "ios:stg": "react-native run-ios --scheme ExampleJenkinsStg --mode StgDebug",
    "ios:stg-release": "react-native run-ios --scheme ExampleJenkinsStg --mode StgRelease",
    "ios:uat": "react-native run-ios --scheme ExampleJenkinsUat --mode UatDebug",
    "ios:uat-release": "react-native run-ios --scheme ExampleJenkinsUat --mode UatRelease",
    "ios:prod": "react-native run-ios --scheme ExampleJenkins --mode ProdDebug",
    "ios:prod-release": "react-native run-ios --scheme ExampleJenkins --mode ProdRelease"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
